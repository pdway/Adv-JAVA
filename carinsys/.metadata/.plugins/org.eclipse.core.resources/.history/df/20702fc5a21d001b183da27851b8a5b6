@Column(name = "Ins_No")
	private String insNo;
	@Column(name = "Provider")
	private String Provider;
	@Column(name = "Ins_Valid_Date")
	private String InsValidDate ;
	@Column(name = "Ins_Holder")
	private String InsHolder ;
	@OneToOne
	@JoinColumn(name = "Lic_no",unique = true)
	private User userv;